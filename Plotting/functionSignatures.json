{
  "_schemaVersion": "1.0.0",
  "SetFont":
  {
     "inputs":
     [
        {"name":"typeface", "kind":"required", "type":["char", "choices= {'Arial', 'Calibri', 'RobotoMono'}"], "purpose":"Name of typeface"},
        {"name":"fontsize", "kind":"required", "type":["numeric"], "purpose":"Size of typeface"}
     ]
  },

  "SymphonicBeeSwarm":
  {
     "inputs":
     [
        {"name":"x", "kind":"required", "type":["numeric"]},
        {"name":"y", "kind":"required", "type":["numeric"]},
        {"name":"color", "kind":"required", "type":["numeric"],"purpose":"[1x3] color matrix"},
        {"name":"point_size", "kind":"required", "type":["numeric"]},
        {"name":"BackgroundType", "kind":"namevalue", "type":["char", "choices={'none', 'violin', 'bar', 'box'}"]},
        {"name":"BoxPercentiles", "kind":"namevalue", "type":["numeric", "choices={'[5 25 75 95]'}"]},
        {"name":"CenterColor", "kind":"namevalue", "type":["numeric", "choices={'[.6 .6 .6]', '[1 .6 .6]', '[.6 1 .6]', '[.6 .6 1]'}"]},
        {"name":"CenterMethod", "kind":"namevalue", "type":["char", "choices={'mean', 'median', 'none'}"]},
        {"name":"DistributionMethod", "kind":"namevalue", "type":["char", "choices={'Histogram', 'KernelDensity'}"]},
        {"name":"MaxPoints", "kind":"namevalue", "type":["numeric"]}
     ]
  }
}
